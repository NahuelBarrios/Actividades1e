#include <stdio.h>
#include <stdlib.h>
#include "array.h"

int initColegio(colegioE pArray[],int limite)
{
	int retorno = -1;

	if(pArray != NULL && limite > 0)
	{
		for(; limite > 0; limite--)
		{
			pArray[limite-1].isEmpty = 1;
		}
		retorno=0;
	}
	return retorno;
}

